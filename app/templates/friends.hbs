{{#with-anything session="service:session" router="router:main" as |_|}}
  {{#round-trip model=model as |__|}}
    {{friends-pagination
      onNextClick=(action goToPage (add page 1))
      onPreviousClick=(action goToPage (sub page 1))
    }}
    {{#unless __.isLoading}}
      {{friends-list
        model=__.model
        onDetailsClick=(action (bind _.router "transitionTo" "index"))
      }}
    {{else}}
      {{partial "loading"}}
    {{/unless}}
  {{/round-trip}}
{{/with-anything}}

{{network-throttle}}
